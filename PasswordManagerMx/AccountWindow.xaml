<Window x:Name="AccountWindow1" x:Class="PasswordManagerMx.AccountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordManagerMx"
        mc:Ignorable="d"
        Title="AccountWindow" Height="367" Width="587.55">
    <Grid Background="#FFB4B4B4">
        <TextBox x:Name="AccountNameEntry" HorizontalAlignment="Left" Height="23" Margin="93,8,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <Label Content="Account Name" HorizontalAlignment="Left" Margin="9,8,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="emailEntry" HorizontalAlignment="Left" Height="23" Margin="93,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="9,36,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="usernameEntry" HorizontalAlignment="Left" Height="23" Margin="93,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="9,65,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="passwordEntry" HorizontalAlignment="Left" Height="23" Margin="93,94,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="11,94,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="etc1Entry" HorizontalAlignment="Left" Height="23" Margin="378,7,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <Label Content="Etc Item #1" HorizontalAlignment="Left" Margin="294,7,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="etc2Entry" HorizontalAlignment="Left" Height="23" Margin="378,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <Label Content="Etc Item #2" HorizontalAlignment="Left" Margin="294,35,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="etc3Entry" HorizontalAlignment="Left" Height="23" Margin="378,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <Label Content="Etc Item #3" HorizontalAlignment="Left" Margin="294,64,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="etc4Entry" HorizontalAlignment="Left" Height="23" Margin="378,93,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <Label Content="Etc Item #4" HorizontalAlignment="Left" Margin="296,93,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="etc5Entry" HorizontalAlignment="Left" Height="23" Margin="378,122,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <Label Content="Etc Item #5" HorizontalAlignment="Left" Margin="296,122,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <Label Content="Random Password Generation" HorizontalAlignment="Left" Margin="9,122,0,0" VerticalAlignment="Top" FontFamily="Cambria Math" FontSize="14"/>
        <Label Content="Seed String" HorizontalAlignment="Left" Margin="11,153,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="seedEntry" HorizontalAlignment="Left" Height="23" Margin="84,153,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="194" Text="&lt;Insert some seed&gt;"/>
        <Label Content="Password Length" HorizontalAlignment="Left" Margin="11,181,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="pwLengthEntry" HorizontalAlignment="Left" Height="23" Margin="109,181,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="169"/>
        <Label Content="Console" HorizontalAlignment="Left" Margin="11,231,0,0" VerticalAlignment="Top" FontFamily="Cambria Math"/>
        <TextBox x:Name="accountEntryConsole" HorizontalAlignment="Left" Height="69" Margin="10,255,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="552"/>
        <Button x:Name="randomPWButton" Content="Generate Random Password" HorizontalAlignment="Left" Margin="109,209,0,0" VerticalAlignment="Top" Width="169" Click="randomPWButton_Click"/>
        <Button x:Name="AddAccountButton" Content="Add Account" HorizontalAlignment="Left" Margin="296,150,0,0" VerticalAlignment="Top" Width="100" Height="48" Click="AddAccountButton_Click" Background="#FF29EE29"/>
        <Button x:Name="updateAccountButton" Content="Update Account" HorizontalAlignment="Left" Margin="431,150,0,0" VerticalAlignment="Top" Width="100" Height="48" Background="#FF279BEC" Click="updateAccountButton_Click"/>
        <Button x:Name="deleteAccountButton" Content="Delete Account" HorizontalAlignment="Left" Margin="431,211,0,0" VerticalAlignment="Top" Width="100" Height="30" Background="#FFFF3B3B" Click="deleteAccountButton_Click"/>

    </Grid>
</Window>
